
<%= form_with model: @user do |form| %>
  <div>
    <%= form.label :username%>
    <%= form.text_field :username, placeholder: "Username"  %>
  </div>

  <div>
    <%= form.label :email%>
    <%= form.text_field :email, placeholder: "example@example.com"%>
  </div>

  <div>
    <%= form.label :password%>
    <%= form.text_field :password, placeholder: "Password"%>
  </div>

  <div>
    <%= form.submit "Edit User" %>
  </div>
  
<% end %>